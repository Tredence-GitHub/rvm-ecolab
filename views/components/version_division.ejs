     console.log("VDATA", <%-  JSON.stringify(versionData)  %>)
</script>

<section id="versions" style="    padding-bottom: 100px;">
<div class="container header-hero-container version" style="background:#fff;">
  <div class="row justify-content-center">

              <div class="col-lg-12">
                  <div class="header-hero-content text-center">
                      <i class="lni lni-list world"></i>
                      <h2 class="header-title wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="0.5s" style="visibility: visible; animation-duration: 1.3s; animation-delay: 0.5s; animation-name: fadeInUp;">User Custom Versions</h2>
                      <p class="text wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="0.8s" style="visibility: visible; animation-duration: 1.3s; animation-delay: 0.8s; animation-name: fadeInUp;">Custom versions of the <%= viewType %> RVM dashboards, with user preferred tonnage and cost estimate versions</p>
                      <div class="link-to"><a href="#"  data-toggle="modal" data-target="#staticBackdrop"><i class="lni lni-plus"></i> &nbsp; Add New Version</a></div>
                    </div>
              </div>
          </div>
    <div class="row justify-content-center">
    <!-- <div class="col-12">
      <p class="txt">Below list of available custom versions can be viewed in the Custom Dashboard (link below)</p>
    </div> -->
    <div class="col-12">
      <div class="table-responsive table-outer">
      <table id="example" class="table table-sm table-bordered" style="width:100%">
      <thead>
        <tr>
          <th class='v-middle' >#</th>
          <th class='v-middle'>Version Name</th>
          <th class='v-middle'>Timeframes Type</th>
          <th class='v-middle'>Requested By</th>
          <th class='v-middle'>Requested Date</th>
          <th class='v-middle'>Status</th>
          <th class='v-middle'>Refresh ETA</th>
          <th class='v-middle'>Expiry_date</th>
        </tr>
      </thead>
      <tbody>
        <!--<td class="green">Processed</td> -->
        <% for(var i=0; i < versionData.length; i++) { %>
           <tr>
             <td><%= i+1 %></td>
             <td><%= versionData[i].versionName %></td>
             <td><%= versionData[i].viewType %></td>
             <td><%= versionData[i].userEmail %></td>
             <td><%= versionData[i].requestedDate %></td>
             <td class='red'><%= versionData[i].status %></td>
             <td><%= versionData[i].refreshEta %></td>
             <td><%= versionData[i].expiryDate %></td>
           </tr>
        <% } %>
        
      </tbody>

    </table>
            </div>
          </div>
          <div class="txt-title viewLinkBox"><a href="">View Report on PowerBI ></a></div>
          <div class="col-12">
            <div style="padding:20px;padding-left:32px; padding-right:32px;">
           <br>
           <br>
           <div class="txt-title">Note - </div>
           <p class="txt">Only 1 version will be accepted per user per day</p>
           <p class="txt">Requests for already existing default and custom versions will not be processed</p>
         </div>
          </div>
        </div>
</div>
</section>
