<div class="modal fade" id="staticBackdrop" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" aria-labelledby="staticBackdropLabel" aria-hidden="true">
<div class="modal-dialog modal-lg modal-dialog-scrollable">
<div class="modal-content">
  <div class="modal-header">
    <h5 class="modal-title" id="staticBackdropLabel"><i class="lni lni-plus"></i> Add New Version  <small style="color:#2593cf;font-weight:600;"> &nbsp;&nbsp; <%= viewType %>  RVM</small></h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
  <div class="modal-body version-modal-body">

    <form id="newRequestForm">
      <div class="form-row">
        <div class="form-group col-md-4">
          <label for="inputPassword4">View Type</label>
           <select id="view"  class="form-control">
              <option disabled value=''>Choose...</option>
              <option value ='standard' selected>Standard</option>
              <option value="forecast">Forecasts </option>
           </select>
        </div>
        <div class="form-group col-md-4">
          <label for="inputPassword4">Timeframes Type</label>
           <select id="timeframesType" class="form-control">
              <option selected value=''>Choose...</option>
              <option value="YTD">YTD</option>
              <option value="MoM">MoM</option>
              <option value="QoQ">QoQ</option>
           </select>
        </div>
      </div>
      <input id="viewType"  type="hidden" name="viewType" value=<%=  viewType %>>
      <div class="" id="newRequestFormModalContent">

      </div>
  </form>
  </div>
  <div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
    <button type="button" class="btn btn-primary" id="continueToSave">Continue</button>
  </div>
</div>
</div>
</div>
